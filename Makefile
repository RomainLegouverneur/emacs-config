install: nlinum auto-complete php-completion anything magit jira tramp

nlinum:
	wget http://elpa.gnu.org/packages/nlinum-1.1.el

auto-complete:
	wget http://cx4a.org/pub/auto-complete/auto-complete-1.3.1.tar.bz2

php-completion:
	wget http://www.emacswiki.org/emacs/download/php-completion.el

anything:
	wget http://www.emacswiki.org/emacs/download/anything.el
	wget http://www.emacswiki.org/emacs/download/anything-config.el
	wget http://www.emacswiki.org/emacs/download/anything-match-plugin.el

magit:
	wget https://github.com/downloads/magit/magit/magit-1.2.0.tar.gz

jira:
	wget http://www.emacswiki.org/emacs/download/jira.el

tramp:
	wget http://ftp.gnu.org/gnu/tramp/tramp-2.0.39.tar.gz